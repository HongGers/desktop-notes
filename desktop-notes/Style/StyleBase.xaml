<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:core="clr-namespace:System;assembly=mscorlib">

    <!--Font-->
    <FontFamily x:Key="FontFamily">Comic Sans MS</FontFamily>

    <core:Double x:Key="SmallFontSize">14</core:Double>
    <core:Double x:Key="NormalFontSize">16</core:Double>
    <core:Double x:Key="LargeFontSize">20</core:Double>

    <!--Color-->
    <Color x:Key="Yellow">#feff99</Color>
</ResourceDictionary>